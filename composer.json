{
	"name": "asinfotrack/yii2-wiki",
	"type": "yii2-extension",
	
	"description": "Yii2-Wiki is a flexible implementation of a wiki for Yii2",
	"keywords": ["yii2", "wiki", "module"],
	
	"license": "MIT",
	"authors": [
		{
			"name": "Pascal Mueller",
			"email": "pascal.mueller@asinfotrack.ch",
			"homepage": "http://www.asinfotrack.ch"
		}
	],
	
	"minimum-stability": "stable",
	"support": {
		"source": "https://github.com/asinfotrack/yii2-wiki",
		"issues": "https://github.com/asinfotrack/yii2-wiki/issues"
	},
	
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "2.0.*",
	  	"yiisoft/yii2-bootstrap": "*",
		"erusev/parsedown": "1.7.*"
	},
	
	"autoload": {
		"psr-4": {
			"asinfotrack\\yii2\\wiki\\": ""
		}
	}
}
